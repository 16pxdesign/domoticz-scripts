return {
	on = {
		devices = {3},
		timer = {},
		variables = {},
		scenes = {},
		groups = {},
		security = {},
		httpResponses = {}
	},
	data = {},
	logger = {},
	execute = function(domoticz, triggeredItem)
	    switch = domoticz.devices(4)
	    if(not switch.active) then
	        	    switch.switchOn()
	        end


	end
}
